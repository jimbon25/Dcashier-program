{
  "version": 2,
  "functions": {
    "backend/dist/index.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/dist/index.js"
    }
  ],
  "installCommand": "cd backend && npm install --include=dev",
  "buildCommand": "cd backend && npm run build"
}
